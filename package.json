{
  "name": "n8n-nodes-nats",
  "version": "1.0.2",
  "description": "NATS Nodes for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/LuxChanLu/n8n-nodes-nats",
  "author": {
    "name": "Hugo Meyronneinc",
    "email": "hugo@lxc.lu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LuxChanLu/n8n-nodes-nats.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NATSApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Nats/Nats.node.js",
      "dist/nodes/JetStream/JetStream.node.js",
      "dist/nodes/JetStream/JetStreamTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/request-promise-native": "~1.0.20",
    "@typescript-eslint/parser": "~6.9",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^3.0.3",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "nats": "^2.17.0"
  }
}
